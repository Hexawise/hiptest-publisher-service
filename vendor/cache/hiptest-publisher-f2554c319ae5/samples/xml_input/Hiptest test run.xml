                  <value>
                    <operation>
                      <left>
                        <var>x</var>
                      </left>
                      <operator>+</operator>
                      <right>
                        <numericliteral>1</numericliteral>
                      </right>
                    </operation>
                  </value>
                </assign>
              </body>
            </while>
            <if>
              <condition>
                <parenthesis>
                  <operation>
                    <left>
                      <var>x</var>
                    </left>
                    <operator>==</operator>
                    <right>
                      <numericliteral>0</numericliteral>
                    </right>
                  </operation>
                </parenthesis>
              </condition>
              <then>
                <step>
                  <result>
                    <template>
                      <var>x</var>
